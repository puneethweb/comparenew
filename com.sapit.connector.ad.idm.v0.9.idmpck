function sapit_CheckAuditId(Par) {

    try {
        var auditID = uGetAuditID();

        var refaudit = uSelect("select refaudit from mxp_audit with (NOLOCK) where auditid = '" + auditID + "'");
        auditID = refaudit;
        var storedValue = uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
        if (storedValue.length == 0) {
            uSetGlobalVar("TEMP_COUNT_AUDITID", auditID);
        } else {
            var temp_audit = uGetGlobalVar("TEMP_COUNT_AUDITID", "");
            if (temp_audit == auditID) {
                //uWarning("in the cpmpare of Audit ID");
            } else {
                uSetGlobalVar("TEMP_COUNT_AUDITID", "");
                uSetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
            }
        }
    } catch (e) {
        UserFunc.uErrMsg(2, "Error on JSript sapit_CheckAuditId " + e.message);
