<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="Script101">
<mx:Code>// 1.0 | D057460 | 2012-12-07 | returns the privilege group name for SAP IDM group assignments
// 1.1 | I818980 | 2013-06-19 | 7.2 adaption
// 1.2 | I818980 | 2013-10-21 | Fix SQL error after 7.2 Upgrade
// 1.3 | D037516 | 2015-12-02 | 8.0 adaption


function Script101(Par) {

    var AuditID = uGetAuditID();
    var CurrentEntryID = uGetEntryID();

    //uWarning("CurrentEntryID---" + CurrentEntryID);

    //uErrMsg(1, CurrentEntryID + " / " +AuditID);
    var priv = uSelect("select mcOtherMSKEY from idmv_link_ext_active with (NOLOCK) where mcThisMSKey = " + CurrentEntryID + " and mcProvAudit = '" + AuditID + "'");
    //uWarning("AuditID---" + AuditID);
    //uWarning("priv---" + priv);
    var userDN = "" + uIS_GetValue(CurrentEntryID, "1", "DNGLOBAL");
    //uWarning("userDN---" + userDN);
    if (priv.length &gt; 0) {
        var techName = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + priv + " and attrname = 'SAPIT_PRIVILEGE_TECHNICAL_NAME'");
        var type = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + priv + " and attrname = 'MX_PRIVILEGE_TYPE'");
        var ret = "cn=" + techName + ",ou=groups,ou=IAM,ou=Resources,DC=globallab,DC=corp,DC=sap";
        //uWarning("type---" + type);
        //uWarning("techName---" + techName);
        //uWarning("ret---" + ret);
        if (type.indexOf("GROUP") == 0) {
            return ret;
        } else {
            uErrMsg(1, "Attention: auditID " + AuditID + " / entry " + CurrentEntryID + " / priv: " + priv);
        }
    } else {
        var endReached = false;
        while (!endReached) {
            var tmp = uSelect("select refaudit from mxp_audit with (NOLOCK) where auditid = '" + AuditID + "'");
            if (tmp.length &gt; 0 &amp;&amp; tmp.indexOf("null") &lt; 0) {
                uWarning("AuditID 33---" + AuditID);
                AuditID = tmp;
                uWarning("AuditID 35---" + AuditID);
            } else
                endReached = true;
            //uErrMsg(1,AuditID + "/" +tmp);
        }


        //priv = uSelect("select top 1 avalue from idmv_ovalue_basic_all with (NOLOCK) where mskey = " + CurrentEntryID + " and AuditID = '" + AuditID + "'");

        priv = uSelect("SELECT top 1 AssignmentMSKEY FROM idmv_linkaudit_basic with (NOLOCK) WHERE  mcNewValidTo IS NULL and UserMSKEY = " + CurrentEntryID + " and AuditId = '" + AuditID + "'");
        //priv = uSelect("SELECT top 1 AssignmentMSKEY FROM idmv_linkaudit_basic WHERE AuditId='" + AuditID + "' AND UserMSKEY=" + CurrentEntryID + " AND UserMSKEY IN (SELECT mskey FROM idmv_ovalue_basic_all with (NOLOCK) WHERE mskey = " + CurrentEntryID + ")");

        //   uWarning("deserializedArray" + deserializedArray);
        uWarning("AuditID 41---" + AuditID);
        uWarning("priv 42---" + priv);


        if (priv.length &gt; 0) {
            var techName2 = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + priv + " and attrname = 'SAPIT_PRIVILEGE_TECHNICAL_NAME'");
            var type2 = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + priv + " and attrname = 'MX_PRIVILEGE_TYPE'");
            var ret2 = "cn=" + techName2 + ",ou=Groups,ou=IAM,ou=Resources,DC=globallab,DC=corp,DC=sap";
            uWarning("type2---" + type2);
            uWarning("techName2---" + techName2);
            uWarning("ret2---" + ret2);

            //uErrMsg(1,"remove: " + ret);
            if (type2.indexOf("GROUP") == 0) {
                return ret2;
            } else {
                uErrMsg(1, "Attention: auditID " + AuditID + " / entry " + CurrentEntryID + " / priv: " + priv);
            }
        }
    }

    uErrMsg(1, "No privilege can be found: auditID " + AuditID + " / entry " + CurrentEntryID + " / priv: " + priv);
    return "";
}</mx:Code>
</mx:Function>
<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapit_assignUserToADSGroups">
<mx:Code>function sapit_assignUserToADSGroups(Par) {

    var auditID = uGetAuditID(); //get current audit ID
    var currentEntryID = uGetEntryID(); // get user MSKEY

    //get the MSKEY of the privilege to be assigned
    var privilegeMSKEY = uSelect("select mcOtherMSKEY from idmv_link_ext_active with (NOLOCK) where mcThisMSKey = " + currentEntryID + " and mcProvAudit = '" + auditID + "'");


    if (privilegeMSKEY.length &gt; 0) {
        //get privilege name by privilege MSKEY
        var privilegeName = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + privilegeMSKEY + " and attrname = 'SAPIT_PRIVILEGE_TECHNICAL_NAME'");

        //get privilege type
        var privilegeType = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + privilegeMSKEY + " and attrname = 'MX_PRIVILEGE_TYPE'");

        //get the LDAP starting point for groups from repository constants
        var ldapStartingPoint = UserFunc.uGetConstant("rep.LDAP_STARTING_POINT_GROUPS");

        //construct the group DN
        var groupDN = "cn=" + privilegeName + "," + ldapStartingPoint;

        if (privilegeType.indexOf("GROUP") == 0) {
            return groupDN;
        } else {
            uErrMsg(1, "Attention: auditID " + auditID + " / entry " + currentEntryID + " / priv: " + privilegeMSKEY);
        }
    } else {
        uErrMsg(1, "No privilege can be found: auditID " + auditID + " / entry " + currentEntryID + " / priv: " + privilegeMSKEY);

    }


    uErrMsg(1, "No privilege can be found: auditID " + auditID + " / entry " + currentEntryID + " / priv: " + privilegeMSKEY);
    return "";

}</mx:Code>
</mx:Function>
