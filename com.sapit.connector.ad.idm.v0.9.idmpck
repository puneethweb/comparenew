// Main function: sapit_getClient

function sapit_getClient(Par) {
    // check parameter is global -&gt; glb
    // check parameter is lab -&gt; lab
    if (Par.split("_")[1] == "GLOBAL")
        return "GLB";
    else
        return Par.split("_")[1];
}</MCSCRIPTDEFINITION>
