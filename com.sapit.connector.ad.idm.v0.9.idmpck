<ALLOWMULTIEVENTS>0</ALLOWMULTIEVENTS>
<BOOLSQL IS_SELF_REFERRING="0">select count(a.cn) as count
from 
mxmc_db.dbo.idmv_entry_simple m with (nolock),
mxmc_db.dbo.idmv_value_ext_active m1 with (nolock),
mxmc_db.dbo.idmv_value_ext_active m2 with (nolock) ,
mxmc_db.dbo.idmv_value_ext_active m3 with (nolock) left join mxmc_db.mxmc_rt_u.sapit_ad_group_all_%$pck.REPO_NAME% as a with (nolock)
on 
m3.SearchValue=a.cn
where m.mcMSKEY=m1.MSKEY and  m2.MSKEY=m1.MSKEY and m3.MSKEY=m2.MSKEY and 
 m.mcEntryType='MX_PRIVILEGE'   and 
 m3.AttrName='SAPIT_PRIVILEGE_TECHNICAL_NAME'
   and
 m2.AttrName='SAPIT_AD_GROUP_TYPE' and m2.SearchValue in('8','-2147483640') 
 and m1.AttrName='SAPIT_AD_ACTIVE' and m1.SearchValue='0' 
 and m3.MSKEY=%MSKEY%</BOOLSQL>
