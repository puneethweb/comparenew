<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapit_deassignUserAD">
<mx:Code>// 1.0 | C5353960 | 2023-23-07 | returns the privilege group name for SAP IDM group assignments


function sapit_deassignUserAD(Par) {
    var currentPriv = 0;
    var auditID = uGetAuditID();
    var currentEntryID = uGetEntryID();

    // uWarning("CurrentEntryID---" + CurrentEntryID);

    var endReached = false;
    while (!endReached) {
        var refAudit = uSelect("select refaudit from mxp_audit with (NOLOCK) where auditid = '" + auditID + "'");
        if (refAudit.length &gt; 0 &amp;&amp; refAudit.indexOf("null") &lt; 0) {
            auditID = refAudit;
        } else {
            endReached = true;
        }
    }

    var storedValue = uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
    // uWarning("TEMP_COUNT_ASSIGNMENTMSKEY----" + uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", ""));
    uSetGlobalVar("TEMP_COUNT_AUDITID", auditID);

    if (storedValue.length &lt;= 0) {
        var tempStoredValueInitial = [];
        queryResPriv = uSelect("SELECT AssignmentMSKEY FROM idmv_linkaudit_basic with (NOLOCK) WHERE  mcNewValidTo IS NULL and  Operation=13 and UserMSKEY = " + currentEntryID + "  and AuditId = '" + auditID + "'");
        tempStoredValueInitial = queryResPriv.split("!!");
        uSetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", tempStoredValueInitial);
        var tempGet = uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
        var tempIni = tempGet.split(",");
        currentPriv = tempIni[0];
    }

    if (storedValue.length &gt; 0) {
        var storedValueTemp = [];
        storedValueTemp = uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
        var tempNext = storedValueTemp.split(",");
        currentPriv = tempNext[0];
    }
    if (currentPriv.length &gt; 0) {
        var privilegeName = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + currentPriv + " and attrname = 'SAPIT_PRIVILEGE_TECHNICAL_NAME'");
        var privilegeType = uSelect("select top 1 avalue from idmv_value_ext_active with (NOLOCK) where mskey = " + currentPriv + " and attrname = 'MX_PRIVILEGE_TYPE'");
        var ldapStartingPoint = UserFunc.uGetConstant("rep.LDAP_STARTING_POINT_GROUPS");
        // uWarning(" UserFunc.uGetConstant " + ldapStartingPoint);
        var groupDN = "cn=" + privilegeName + "," + ldapStartingPoint;

        if (privilegeType.indexOf("GROUP") == 0) {
            var tempToDelete = uGetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", "");
            // uWarning(" tempToDelete " + tempToDelete);
            var temp = tempToDelete.split(",");
            temp.splice(0, 1);
            // uWarning("temp.length -----" + temp.length);
            uSetGlobalVar("TEMP_COUNT_ASSIGNMENTMSKEY", temp);
            return groupDN;
        } else {
            uErrMsg(1, "Attention: auditID " + auditID + " / entry " + currentEntryID + " / priv: " + currentPriv);
        }
    }


    // Write code to clear both TEMP_COUNT and TEMP_COPUNT_Audit  setGlobalValue(TEMP_COUNT_AUDITID,"");
    uErrMsg(1, "No privilege can be found: auditID " + auditID + " / entry " + currentEntryID + " / priv: " + currentPriv);
    return "";
}</mx:Code>
