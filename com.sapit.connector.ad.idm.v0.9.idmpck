// Main function:sapit_getJobVariable

function sapit_getJobVariable(Par) {
    var parArr = Par.split("!!");
    var jobVariable = parArr[0];
    var defaultValue = parArr[1];
    var value = uGetUserVar(jobVariable, defaultValue);
    return value;
}</MCSCRIPTDEFINITION>
